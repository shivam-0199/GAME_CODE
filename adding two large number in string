#include <iostream>
#include<bits/stdc++.h> 
using namespace std;
int main() {
    // your code goes here
    int t;
    string a;
    cin>>t;
    for(int i=0;i<t;i++){
        cin>>a;
        string b="1";
        string x="";
        int len=a.length();
        reverse(a.begin(),a.end());
        int carry=0;
        for(int j=0;j<1;j++){
            int sum=((a[j]-'0')+(b[j]-'0')+carry);
             x.push_back(sum%10 + '0');
             carry=carry/10;
        }
         for(int j=1;j<len;j++){
            int sum=((a[j]-'0')+carry);
             x.push_back(sum%10 + '0');
             carry=carry/10;
        }
        if(carry){
            x.push_back(carry +'0');
        }
        reverse(x.begin(), x.end());
        cout<<x;
            
        
     
        
    }
    return 0;
}
